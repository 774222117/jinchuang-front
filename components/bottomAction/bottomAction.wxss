
.cart-btmBox{
  width: 100%;
  height: 124rpx;
  position: fixed;
  background-color: white;
  z-index: 99;
  display: flex;
  padding-bottom:  env(safe-area-inset-bottom);
  box-sizing: content-box;
}
.bt120{
  bottom: 120rpx;
}
.bt20{
  bottom:10rpx;
}
.bt0{
  bottom:0;
}
/* 门店选择列表 */
.cu-dialog.shopCar{
  border-top-left-radius: 25px !important;
  border-top-right-radius: 25px !important;
}
.radius50{border-radius: 50%;text-align: center;line-height: 112rpx;font-size: 50px;}
.xl{width: 100%;height: 100%;margin: 0;position: relative}
.cart-btmBox .text-money11{
  opacity: 1;
  font-size: 36rpx;
  font-family: DIN, DIN-Medium;
  font-weight: 500;
  text-align: left;
  color: #f35437;
}
.cart-btmBox .text-money11-deci{
  opacity: 1;
  font-size: 28rpx;
  font-family: DIN, DIN-Medium;
  font-weight: 500;
  text-align: left;
  color: #f35437;
}
.cart-btmBox .text-money22{
  opacity: 0.55;
  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  text-align: left;
  color: #141414;
}
.placeAnOrder{
  height: 112rpx;
  border-radius:0;
  line-height: 112rpx;
  font-size:32rpx;
  font-weight:bold;
  color:rgba(255,255,255,1);
  font-family:PingFangSC-Semibold,PingFang SC;
}
.btmLine{width:134px;height:5px;background:rgba(0,0,0,1);border-radius:100px;margin: 20rpx auto}
.left-cart-btn{
  background-color: white;
  width: 15%;
  height:100%;
}
.left-cart-btn .cart-img{

  height:100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cu-tag.badge {
  border-radius: 200rpx;
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  font-size: 18rpx;
  height: 26rpx;
  color: var(--white);
}
.priceInfo{
  background-color: rgba(255, 255, 255, 1);
  display: flex;
  width: 55%;
  flex-direction: column;
  justify-content: center;
}
.car-icon-bg{
  background-image:linear-gradient(135deg,rgba(255, 59, 84, 1) 0%,rgba(255,178,111,1) 100%);
}
.cuIcon-cart ::before{
  color: white;
}
.car-left-bg{
  width: 80rpx;
  height: 112rpx; 
  background-color: white;
  position: absolute;
  top:0;
  z-index: -1;
  left:56rpx;
}
.placeBtn{
  height:100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.placeBtn .pay-btn{
  width: 200rpx;
  height: 64rpx;
  opacity: 1;
  background: #1c6a4f;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.pay-btn .tex1{
  opacity: 1;
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  text-align: left;
  color: #ffffff;
}
.shopCart-Item{
  display: flex;
  flex-direction: row;
  padding: 24rpx 30rpx;
  background-color: white;
  position:relative; 
}
.shopCar-LeftImg{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.shopCar-LeftImg image{
  width: 120rpx;
  height: 120rpx;
}
.shopCar-RightBox{
  display: flex;
  flex-direction: column;
  width: 378rpx;
  flex-shrink:0;
  margin-left: 20rpx;
}
.shopCar-PriceBox{
  display: flex;
  align-items: center;
  margin-top: 20rpx;
}
.shopCar-topTitle{
  display: flex;
  justify-content: start;
  opacity: 1;
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  text-align: left;
  color: #323232;
  overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-break: break-all; /* 追加这一行代码 */
  text-align: left;
}
.shopCar-tag{
  display: flex;
}
.shopCar-tag .tag{
  width: 116rpx;
  height: 36rpx;
  opacity: 1;
  background: #f3f3f3;
  border-radius: 8rpx;

  opacity: 0.69;
  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  text-align: center;
  color: #141414;
  margin-right: 8rpx;
  margin-top: 6rpx;
}
.shopCar-countBox{
  display: flex;
  align-items:flex-end;
}

 .shopCar-minusBtn,.shopCar-plusBtn{
  width: 44rpx;
  height: 44rpx;
  background-color: #FF3B30;
  color: white;
  text-align: center;
  line-height: 44rpx;
  border-radius: 50%;
  font-size: 34rpx;
}
 .shopCar-image{
  width: 52rpx;
  height: 52rpx;
}
.shopCar-goosNum{
  width: 84rpx;
  height: 52rpx;
  text-align: center;
  line-height: 52rpx;
  font-size: 32rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  text-align: center;
  color: #3d444d;
}

.text-shopCarMoney1{
  opacity: 1;
  font-size: 30rpx;
  font-family: DIN, DIN-Medium;
  font-weight: 500;
  text-align: left;
  color: #f35437;
  margin-right: 6rpx;
  }
  .text-shopCarMoney1-deci{
    opacity: 1;
    font-size: 28rpx;
    font-family: DIN, DIN-Medium;
    font-weight: 500;
    text-align: left;
    color: #f35437;
  }
.text-shopCarMoney2{
  opacity: 0.18;
  font-size: 24rpx;
  font-family: DIN, DIN-Medium;
  font-weight: 500;
  text-decoration:  line-through;
  text-align: left;
  color: #323232;
}
.shopItemList .rightBox .countBox{
  justify-content: space-between;
  align-items: center;
  margin-top: 10rpx;
}
.line{
/*width: 150rpx;*/
height: 1rpx;
background-color: #D8D8D8;
}
.shopCart-Item::after{
  background-color: #D8D8D8;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 1px;
  content: '';
  -webkit-transform: scaleY(.5);
}
.shopCart-Item:last-child:after{
  height: 0;
}

/* 地址选择 */
.cu-dialog.addressBox{
  border-radius:25px !important;
}
.addressTitle{
  height: 138rpx;
}
.addressItem{
  width: 100%;
  min-height:120rpx;
  align-items: center;
  border-bottom: 1px solid rgba(0,0,0,0.1)
}
.addressItem .addressItemLeftImg{
  width: 80rpx;
  height: 80rpx;
}
.addressItem .addressItemLeftImg image{
  width: 100%;
  height: 100%;
  display: block;
}
.addressItem .addressItemMiddleBox{
  width: 600rpx;
  text-align: left;
  font-weight: bold;
  font-size:14px;
}
.addressItem .addressItemMiddleBox .text-sty2{
  width: 100%;
  height: auto;
  font-size:12px;
  line-height: 34rpx;
  color:rgba(136,136,136,1);
  margin-top: 10rpx;
  font-weight: normal;
  display: flex;
}
.addressItem .addressItemMiddleBox .text-sty2 view:nth-child(1){
  width: auto;
  height: 34rpx;
}
.addressItem .addressItemMiddleBox .text-sty2 view:nth-child(2){
  width: 420rpx;
  height: 34rpx;
  display: inline-block;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  margin-left: 20rpx;
}