/* pages/cardVoucherList/cardVoucherList.wxss */
page{padding-top: 38rpx;}
/* 卡片 */
.cards{
  width: 662rpx;
  height: 132rpx;
  margin:0 auto 20rpx;
  position: relative;
}
.card_text1{
  width: 228rpx;
  height: 44rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: #ffffff;
  position: absolute;
  left: 128rpx;
  top: 42rpx;
}
.card_text2{
  width: 200rpx;
  height: 34rpx;
  font-size: 24rpx;
  color: #e5dabb;
  line-height: 24rpx;
  position: absolute;
  left: 408rpx;
  top: 88rpx;
}
.card_text3 {
  display: flex;
  align-items: center;
  width: 20rpx;
  /* height: 132rpx; */
  font-size: 20rpx;
  color: #ffffff;
}
/* 弹框 */
.models{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(20,20,20,0.63);
}
.models .content{
  width: 606rpx;
  height: 480rpx;
 overflow: hidden;
  background: #ffffff;
  border-radius: 20rpx;
  position: relative;
  left: 0;
  right: 0;
  top: 300rpx;
  margin: 0 auto;
}
.models .content .title{
  font-size: 32rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: bold;
  text-align: center;
  color: #141414;
  margin: 34rpx auto;
}
/* 实体卡 */
.models .content .entityCard{}
/* 电子卡 */
.models .content .electronCard{}

.pubsty{
  width: 532rpx;
  height: 90rpx;
  border-bottom: 2rpx solid rgba(112,112,112,0.19);
  margin: 0 auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.texts{
  
}
.texts .text1{
  font-size: 28rpx;
  color: #141414;
}
.texts .text2{
  font-size: 20rpx;
  color: #d6912d;
}
.radio{
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
}

.center{
  width: 144rpx;
  height: 52rpx;
  background: #1c6a4f;
  border-radius: 8rpx;
  font-size: 24rpx;
  text-align: center;
  line-height: 52rpx;
  color: #ffffff;
  margin: 100rpx auto 0;
}
.close{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 22rpx;
  right: 22rpx;
}

.imgsty{width: 100%;height: 100%;display: block;}

.card_exchange {
  width: 120rpx;
  height: 132rpx;
  position: absolute;
  right: 0;
  top: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}

