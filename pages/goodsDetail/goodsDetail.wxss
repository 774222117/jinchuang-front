.mianBox {
  display: flex;
  flex-direction: column;
  width: 100%;
}

.goodsInfo {
  width: 704rpx;
  /* height: 266rpx; */
  padding: 20rpx 22rpx 24rpx 18rpx;
  box-sizing: border-box;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  /* justify-content: space-between; */
  margin: 18rpx auto;
  border-radius: 20rpx;
}

.goodsInfo_price {
  width: 100%;
  height: 44rpx;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}

.goodsInfo_priceLeft {
  display: flex;
  align-items: flex-end;
}

.goodsInfo_text1 {
  font-size: 36rpx;
  height: 44rpx;
  line-height: 44rpx;
  font-family: DIN, DIN-Medium;
  font-weight: 500;
  color: #f35437;
  vertical-align: bottom;
  margin-right: 8rpx;

}

.goodsInfo_text2 {
  height: 34rpx;
  line-height: 34rpx;
  font-size: 28rpx;
  opacity: 0.18;
  font-family: DIN, DIN-Medium;
  font-weight: 500;
  text-decoration: line-through;
  color: #323232;
}

.goodsInfo_text3 {
  height: 34rpx;
  line-height: 34rpx;
  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  color: #9e9e9e;
  margin-right: 30rpx;
}

.goodsInfo_tagBox {
  width: 100%;
  display: flex;
  align-items: center;
  margin: 12rpx 0 14rpx;
}

.goodsInfo_tag {
  height: 32rpx;
  display: flex;
  align-items: center;
  border-radius: 16rpx;
  padding: 0 16rpx;
  box-sizing: border-box;
  border: 2rpx solid #e2b472;
  font-size: 20rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  color: #e1ab5d;
  margin-right: 8rpx;
}

.goodsInfo_title {
  line-height: 44rpx;
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Heavy;
  font-weight: 800;
  color: #141414;
}

.goodsInfo_describe {
  /* height: 34rpx; */
  line-height: 34rpx;
  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  color: #e1ab5d;
}

.goodsDetails {
  width: 704rpx;
  height: 80rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  background-color: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 18rpx auto 0;
  border-radius: 20rpx;
  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Medium;
  font-weight: 500;
  color: #141414;
}

.goodsDetails_left {
  display: flex;
  align-items: center;
}

.goodsDetails_tag {
  width: 90rpx;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  background: rgba(225, 171, 93, 0.34);
  font-size: 20rpx;
  font-family: PingFang SC, PingFang SC-Medium;
  font-weight: 500;
  color: #d6912d;
  border-radius: 6rpx;
  margin-right: 14rpx;
}

.goodsDetails_img {
  width: 32rpx;
  height: 32rpx;
}

.goodsDetails_title {
  width: 100%;
  height: 92rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 36rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  color: #141414;
}

.goodsDetails_line {
  width: 58rpx;
  height: 2rpx;
  opacity: 0.22;
  background: #141414;
  margin: 0 14rpx;
}

/* 详情 */
.details {
  width: 100%;
  height: 1000rpx;
}

.detail-info {
  width: 100%;
  background-color: #ffffff;
  height: 572rpx;
}

.detail-info .info-item {
  width: 668rpx;
  padding: 25rpx 5rpx;
  margin: 0 auto;
  border-bottom: 2rpx dashed #707070;
  display: flex;
  justify-content: space-between;
}

.detail-info .info-item:last-child {
  border-bottom: none;
}

.info-item .item-title {
  width: 70%;
  opacity: 1;
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  text-align: left;
  color: rgba(28, 106, 79, 1);
  line-height: 40rpx;
  letter-spacing: 1rpx;
}

.info-left {
  flex-direction: row;
  flex-wrap: wrap;
  display: flex;
  align-content: space-between;
}

.info-right {
  display: flex;
  align-items: center;
}

.info-item .item-tip {
  opacity: 1;
  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  text-align: left;
  color: rgba(20, 20, 20, 1);
  line-height: 40rpx;
  letter-spacing: 1px;
}

/* 详情标题 */
.details .detailsTitle {
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 32rpx;
  font-family: PingFang SC, PingFang SC-Semibold;
  font-weight: 600;
  color: rgba(0, 0, 0, 0.85);
  text-indent: 40rpx;
  background-color: #ffffff;
}

.footBox {
  width: calc(100% - 68rpx);
  height: 130rpx;
  padding: 0 22rpx 0 46rpx;
  padding-bottom: env(safe-area-inset-bottom);
  box-sizing: content-box;
  background-color: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-shadow: 0 -2rpx 8rpx 0 rgba(0, 0, 0, 0.1);
}

.footTabBox {
  width: 142rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.footTab {
  display: flex;
  width: auto !important;
  flex-direction: column;
  align-items: center;
  position: relative;
  padding: 0;
  background-color: transparent;
  margin: 0 !important;
}

.footTab_img {
  width: 52rpx;
  height: 52rpx;
}

.footTab_text {
  height: 28rpx;
  line-height: 28rpx;
  font-size: 20rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  color: #141414;
}

/* 购物车角标 */
.goodsNum {
  width: 36rpx;
  height: 36rpx;
  background: #ff3b30;
  border: 2rpx solid #ffffff;
  border-radius: 50%;
  position: absolute;
  top: -10rpx;
  right: -10rpx;
  font-size: 22rpx;
  text-align: center;
  line-height: 36rpx;
  color: #ffffff;
}

.footBtnBox {
  display: flex;
  height: 68rpx;
  width: 480rpx;
}

.footBtn {
  height: 100%;
  width: 240rpx;
  box-sizing: border-box;
  border-radius: 34rpx 0 0 34rpx;
  line-height: 68rpx;
  text-align: center;
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  color: #ffffff;

}

.mask {
  position: fixed;
  z-index: 4;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(50, 50, 50, 0.78);
}

/* 弹窗 */
.dialog {
  display: flex;
  z-index: 5;
  width: 100%;
  height: 0;
  overflow: hidden;
  position: fixed;
  bottom: -100%;
  left: 0;
  transition: all 0.5s;
  align-items: flex-end;
}

.dialogMainShow {
  bottom: 0;
  height: 100%;
}

.dialogMain {
  width: 100%;
  /* height: 802rpx; */
  border-radius: 20rpx 20rpx 0 0;
  background-color: #fff;
  box-sizing: border-box;
  position: relative;
  display: flex;
  flex-direction: column;
  transition: all 0.5s;
}

.closeBtn {
  position: absolute;
  top: 22rpx;
  right: 22rpx;
  width: 44rpx;
  height: 44rpx;
  z-index: 8;
}

.dialog_head {
  width: calc(100% - 46rpx);
  height: 186rpx;
  padding: 28rpx 0 38rpx;
  box-sizing: border-box;
  margin: 0 auto;
  border-bottom: 2rpx solid rgba(112, 112, 112, .13);
  display: flex;
}

.dialog_headDetails {
  margin: 18rpx 0 0 18rpx;
  display: flex;
  height: 44rpx;
  align-items: flex-end;
}

.dialog_headText1 {
  height: 44rpx;
  line-height: 44rpx;
  font-size: 36rpx;
  font-family: DIN, DIN-Medium;
  font-weight: 500;
  color: #F35437;
}

.dialog_headText2 {
  font-size: 28rpx;
  font-family: DIN, DIN-Medium;
  font-weight: 500;
  color: rgba(50, 50, 50, .18);
  text-decoration: line-through;
  margin-left: 8rpx;
}

.dialog_body {
  width: 100%;
  flex: 1;
  display: flex;
  flex-direction: column;
  padding: 24rpx 23rpx 36rpx;
  box-sizing: border-box;
}

.dialog_bodyText1 {
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  color: #141414;
  height: 40rpx;
  line-height: 40rpx;
  margin-bottom: 16rpx;
}

.dialog_tagBox {
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  /* height: 56rpx; */
  /* padding-bottom: 22rpx; */
  margin-bottom: 24rpx;
  box-sizing: content-box;
  border-bottom: 2rpx solid rgba(112, 112, 112, .13);
}

.dialog_tagList {
  /* width: 116rpx; */
  padding: 0 26rpx;
  height: 56rpx;
  line-height: 56rpx;
  background-color: #F3F3F3;
  border-radius: 12rpx;
  text-align: center;
  font-size: 24rpx;
  font-family: DIN, DIN-Regular;
  font-weight: 400;
  margin-right: 16rpx;
  margin-bottom: 22rpx;
}

.active {
  color: #F37920;
  border-color: #F37920;
}

.dialog_bodyNum {
  width: 100%;
  height: 52rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.dialog_bodyText2 {
  margin-left: 12rpx;
  font-size: 22rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  color: rgba(20, 20, 20, .53);
}

.numBtn {
  width: 160rpx;
  height: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 32rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  color: #3d444d;
}

.numBtn image {
  width: 52rpx;
  height: 52rpx;
}

.bottomBtnBox {
  width: calc(100% - 24rpx);
  height: 68rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  padding-right: 24rpx;
  padding-bottom: env(safe-area-inset-bottom);
  box-sizing: content-box;
  margin-bottom: 28rpx;
}

.btnBox {
  width: 240rpx;
  height: 100%;
  background-color: #d6912d;
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  text-align: center;
  color: #ffffff;
  line-height: 68rpx;
  border-radius: 34rpx 0 0 34rpx;
}


