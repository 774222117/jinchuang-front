/* pages/arrondi/arrondi.wxss */
page {
  background: #f5f5f5;
}

.searchMain {
  width: 100%;
  height: 136rpx;
  display: flex;
  align-items: center;
  background-color: white;
  /* margin-bottom: 24rpx; */
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
}

.search {
  width: 702rpx;
  height: 60rpx;
  margin: 0 auto;
  background: #eeeeee;
  border-radius: 12rpx;
  display: flex;
  align-items: center;
}

.search .search_icon {
  width: 40rpx;
  height: 40rpx;
  margin-left: 20rpx;
  margin-right: 4rpx;
}

.search .search_input {
  width: 630rpx;
  height: 100%;
}

.search .search_input input {
  width: 100%;
  height: 100%;
  line-height: 1;
}

.placeholderSty {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(50, 50, 50, 0.18);
}

/* banner */
.banner {
  width: 702rpx;
  /* height: 230rpx; */
  border-radius: 20rpx;
  margin: 0 auto;
  padding: calc(24rpx + 136rpx) 0 24rpx;
}

/* 分类icon classify */
.classify {
  width: 702rpx;
  height: 224rpx;
  background: #ffffff;
  border-radius: 20rpx;
  margin: 0 auto;
}

.swiper-item {
  height: 198rpx;
  display: flex;
  align-items: center;
  /* justify-content: space-between; */
}

.classify .categoryItme {
  width: 118rpx;
  margin-right: 28rpx;
}

.classify .categoryItme .categoryItmeImg {
  width: 118rpx;
  height: 118rpx;
  border-radius: 50%;
  margin: 0 auto 4rpx;
  overflow: hidden;
}

.classify .categoryItme .categoryItmeTitle {
  width: 100%;
  height: 34rpx;
  text-align: center;
  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  color: #000000;
}

.lines {
  height: 4rpx;
  background: #ebdcb3;
  border-radius: 2rpx;
  margin: 0 auto;
  position: relative;
  overflow: hidden;
}

.linesActive {
  position: absolute;
  top: 0;
  background-color: #1C6A4F;
  border-radius: 2rpx;
  width: 40rpx;
  height: 4rpx;
}


/* 品牌甄选 */
.brand_title {
  width: max-content;
  height: 93rpx;
  margin-left: 24rpx;
  padding: 36rpx 0 27rpx;
}

.brand_title .brand_title_imgs {
  width: 160rpx;
  height: 32rpx;
}

.brand_contentBox {
  padding-bottom: 34rpx;
}

.brand_content {
  width: 702rpx;
  height: 306rpx;
  box-sizing: border-box;
  background: linear-gradient(180deg, rgba(228, 175, 116, 0.34), rgba(228, 187, 141, 0.15));
  border-radius: 20rpx;
  margin: 0 auto;
  padding: 24rpx 0 24rpx 22rpx;
}

.brand_content_items {
  width: 100%;
  height: 100%;
}

.brand_item {
  width: 202rpx;
  height: 258rpx;
  background: #ffffff;
  border-radius: 20rpx;
  margin-right: 16rpx;
}

.brand_item_img {
  width: 202rpx;
  height: 202rpx;
}
.brand_item_img image{
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}

.brand_item_text {
  width: clac(100% - 40rpx);
  height: 34rpx;
  margin: 8rpx 20rpx 0;
  font-size: 24rpx;
  font-weight: bold;
  color: #323232;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

