.seckill_item_name {
  width: 100%;
  height: 66rpx;
  font-size: 24rpx;
  color: #323232;
  margin-top: 12rpx;
  margin-bottom: 24rpx;
  white-space: normal
}

.seckill_item_price {
  width: 100%;
  height: 44rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.seckill_item_price .prices {
  font-weight: bold;
  color: #f35437;
}

.seckill_item_price .prices text:nth-of-type(1) {
  font-size: 36rpx;
}

.seckill_item_price .prices text:nth-of-type(2) {
  font-size: 26rpx;
}

.seckill_item_price .add {
  width: 30rpx;
  height: 30rpx;
  border: 2rpx solid #1c6a4f;
  border-radius: 6rpx;
}

.seckill_item_price .add::before {
  content: '+';
  width: 100%;
  height: 100%;
  display: block;
  color: #1c6a4f;
  text-align: center;
  line-height: 26rpx;
}

/* 热门标题 样式1 */
.titles_sty1 {
  width: calc(100% - 24rpx);
  height: 98rpx;
  margin-left: 24rpx;
  overflow: hidden;
}

.titles_sty1_items {
  margin-right: 50rpx;
  line-height: 98rpx;
  font-size: 28rpx;
}

.active_sty1 {
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: bold;
  color: #1c6a4f;
}

/* 热门标题 样式2 */
.classify_title {
  max-width: 100%;
  height: 112rpx;
  box-sizing: border-box;
  overflow: hidden;
  padding: 0 20rpx 32rpx 24rpx;
}

.classify_titleFixed {
  width: 100%;
  max-width: 100%;
  height: 112rpx;
  box-sizing: border-box;
  overflow: hidden;
  padding: 0 20rpx 32rpx 24rpx;
  position: fixed;
  top: 136rpx;
  left: 0;
  z-index: 10;
  background-color: #fff;
}

.classify_view {
  width: 100%;
  height: 100%;
}

.title_item {
  width: 158rpx;
  height: 80rpx;
  position: relative;
}

.title_item view:nth-of-type(1) {
  font-size: 28rpx;
  font-weight: bold;
  text-align: center;
  color: #323232;
}

.title_item view:nth-of-type(2) {
  font-size: 24rpx;
  text-align: center;
  color: rgba(50, 50, 50, 0.53);
}

