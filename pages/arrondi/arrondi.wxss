  color: #ffffff;
  margin: 0 auto;
}

/* 分类商品更多商品 */
.moreContents {
  width: 702rpx;
  margin: 0 auto;
  /* display: flex;
  flex-wrap: wrap;
  justify-content: space-between; */
}

.moreContents .left {
  float: left;
}

.moreContents .right {
  float: right;
}

/* 单个商品 */
.moreContents .items {
  width: 342rpx;
  box-sizing: border-box;
  background: #ffffff;
  border-radius: 20rpx;
  padding: 20rpx;
  margin-bottom: 18rpx;
}

.moreContents .items .items_imgs {
  width: 302rpx;
  height: 302rpx;
  margin-bottom: 6rpx;
}

.moreContents .items .items_text {
  font-size: 28rpx;
  font-weight: bold;
  color: #323232;
  line-height: 34rpx;
}

.moreContents .items .items_sold {
  font-size: 24rpx;
  color: rgba(50, 50, 50, 0.66);
  line-height: 34rpx;
  margin-bottom: 14rpx;
}

.moreContents .items .labels {
  width: 100%;
  height: 32rpx;
  display: flex;
  margin-bottom: 30rpx;
}

.moreContents .items .labels .labels_item {
  width: max-content;
  height: 32rpx;
  padding: 0 16rpx;
  border: 2rpx solid #e2b472;
  border-radius: 18rpx;
  font-size: 20rpx;
  text-align: center;
  color: #e1ab5d;
  line-height: 32rpx;
  margin-right: 8rpx;
}

.moreContents .items .items_price {
  width: 100%;
  height: 44rpx;
  display: flex;
  align-items: center;
}

.items_price view:nth-of-type(1) {
  font-size: 36rpx;
  font-weight: bold;
  color: #F35437;
  margin-right: 12rpx;
}

.items_price view:nth-of-type(1) text:nth-of-type(1) {
  font-size: 36rpx;
}

.items_price view:nth-of-type(1) text:nth-of-type(2) {
  font-size: 24rpx;
}

.items_price view:nth-of-type(2) {
  font-size: 28rpx;
  font-weight: bold;
  text-decoration: line-through;
  text-align: left;
  color: rgba(50, 50, 50, 0.18);
}

.items_price view:nth-of-type(3) {
  width: 44rpx;
  height: 44rpx;
  margin-left: auto;
}



.imgsty {
  width: 100%;
  height: 100%;
  display: block;
}

.scroll-view_H {
  width: 100%;
  white-space: nowrap;
}

.scroll-view-item_H {
  display: inline-block;
}

.textOver {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.textOver2 {
  width: 100%;
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box;
  /** 对象作为伸缩盒子模型显示 **/
  -webkit-box-orient: vertical;
  /** 设置或检索伸缩盒对象的子元素的排列方式 **/
  -webkit-line-clamp: 2;
  /** 显示的行数 **/
  overflow: hidden;
  /** 隐藏超出的内容 **/

  font-size: 22rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  color: #323232;
  line-height: 34rpx;
}

.more_icon {
  width: 24rpx;
  height: 24rpx;
  display: block;
}

.more_icon_parent {
  display: flex;
  align-items: center;
}

