/* pages/arrondi2/arrondi2.wxss */

.top {
  width: 100%;
  /* height: 504rpx; */
  margin-bottom: 24rpx;
}

/* 超值优惠券 */
.discountCard {
  width: 702rpx;
  height: 220rpx;
  padding: 20rpx;
  background: #ffffff;
  border-radius: 20rpx;
  margin: 0 auto 24rpx;
}

.discountCard .discountCard_title {
  width: 100%;
  height: 40rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  margin-bottom: 24rpx;
}

.discountCard .discountCard_title .discountCard_title_text1 {
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: bold;
  color: #1c6a4f;
  margin-right: 10rpx;
}

.discountCard .discountCard_title .discountCard_title_text2 {
  width: 86rpx;
  height: 28rpx;
  font-size: 20rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  color: #ffffff;
  background: #d6912d;
  border-radius: 6rpx;
  text-align: center;
}

.discountCard .discountCard_title .discountCard_title_text3 {
  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  color: #323232;
  margin-left: auto;
  display: flex;
  align-items: center;
  opacity: 0.53;
}

.discountCard .discountCard_title .discountCard_title_text3 .discountCard_title_text3_icon {
  width: 30rpx;
  height: 28rpx;
  margin-left: 0rpx;
}

/* 卡片 */
.discountCard_content {
  width: 100%;
  height: 111rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.cardItem {
  width: 320rpx;
  height: 111rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  /* 此处切换背景图片 */
  background-color: lightgreen;
}

.cardItem .cardItem_left {
  width: 250rpx;
  height: 111rpx;
}

.cardItem .cardItem_left .cardItem_left_top {
  height: 70rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.cardItem .cardItem_left .cardItem_left_btm {
  display: flex;
  align-items: center;
  justify-content: center;
}

.cardItem .cardItem_left .cardItem_left_btm .cardItem_left_btm_lines {
  width: 36rpx;
  height: 2rpx;
  background-color: #e5d19b;
  margin: 0 6rpx;
}

.cardItem .cardItem_right {
  width: 70rpx;
  height: 92rpx;
  color: #ffffff;
  text-align: center;
  line-height: 92rpx;
}

/* 下面的元素 */
.btmItems {
  width: 702rpx;
  height: auto;
  overflow: hidden;
  margin: 0 auto;
  /* margin-bottom:124rpx;
  margin-bottom:calc(124rpx + constant(safe-area-inset-bottom));
  margin-bottom:calc(124rpx + env(safe-area-inset-bottom)); */
}

.btmItems .item {
  width: 222rpx;
  height: 380rpx;
  background: #ffffff;
  border-radius: 20rpx;
  float: left;
  margin-right: 18rpx;
  margin-bottom: 18rpx;
  padding: 16rpx 20rpx 0;
}

.btmItems .item:nth-of-type(3n) {
  margin-right: 0;
}

.btmItems .item .item_img {
  width: 182rpx;
  height: 182rpx;
  margin-bottom: 8rpx;
}

.btmItems .item .item_text {
  width: 100%;
  height: 74rpx;
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: bold;
  color: #323232;
  line-height: 34rpx;
  margin-bottom: 32rpx;
}

.btmItems .item .item_price {
  width: 100%;
  height: 44rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.item_price .add {
  width: 30rpx;
  height: 30rpx;
  border: 2rpx solid #1c6a4f;
  border-radius: 6rpx;
}

.item_price .add::before {
  content: '+';
  width: 100%;
  height: 100%;
  display: block;
  color: #1c6a4f;
  text-align: center;
  line-height: 26rpx;
}

.btmItems .item .item_price .priceSty {
  font-size: 34rpx;
  font-family: DIN, DIN-Medium;
  color: #f35437;
}

/* 底部 */
.footer {
  width: 100%;
  min-height: 124rpx;
  background: #ffffff;
  box-shadow: 0px -4rpx 12rpx 0px rgba(0, 0, 0, 0.06);
  position: fixed;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  padding-left: 24rpx;
  padding-right: 24rpx;
}

.leftIcon {
  width: 66rpx;
  height: 66rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}

.price_text {
  width: 320rpx;
  height: 66rpx;
}

.price_text .price_text1 {
  font-size: 36rpx;
  font-weight: bold;
  color: #f35437;
}

.price_text .price_text2 {
  font-size: 24rpx;
  color: rgba(20, 20, 20, 0.56);
}

.footer .btms {
  width: 200rpx;
  height: 64rpx;
  background: #1c6a4f;
  border-radius: 10rpx;
  font-size: 28rpx;
  color: #ffffff;
  text-align: center;
  line-height: 64rpx;
  margin-left: auto;
}



.imgsty {
  width: 100%;
  height: 100%;
  display: block;
}

.scroll-view_H {
  width: 100%;
  white-space: nowrap;
}

.scroll-view-item_H {
  display: inline-block;
}

.ft_w {
