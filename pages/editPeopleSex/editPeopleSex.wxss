.cardBox {
  width: 702rpx;
  display: flex;
  flex-direction: column;
  margin: 24rpx auto 0;
  border-radius: 20rpx;
  background-color: #fff;
  padding: 0 16rpx;
}

.list {
  width: 100%;
  height: 95rpx;
  line-height: 95rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  color: #141414;
}

.listIcon {
  width: 32rpx;
  height: 32rpx;
  margin-left: 10rpx;
}

.line {
  width: 100%;
  height: 2rpx;
  opacity: 0.1;
  background-color: #707070;
}