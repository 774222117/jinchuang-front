/* pages/feedback/feedback.wxss */
page {
  background: #f5f5f5;
}
.feedBackText {
  width: 140rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  text-align: left;
  color: #323232;
  margin: 34rpx 0 0 24rpx;
}
/* 输入框 */
.feedBackBox {
  width: 702rpx;
  height: 288rpx;
  background: #ffffff;
  border-radius: 20rpx;
  margin: 24rpx 0 0 24rpx;
  overflow: hidden;
}
.input {
  width: 672rpx;
  height: 264rpx;
  margin: 24rpx 0 0 24rpx;
}
/* 添加图片 */
.addPic {
  width: 702rpx;
  height: 268rpx;
  background: #ffffff;
  border-radius: 20rpx;
  margin: 24rpx 0 0 24rpx;
  overflow: hidden;
}
.addPicText {
  width: 224rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  text-align: left;
  color: #000000;
  margin: 24rpx 0 0 24rpx;
}
.addPicBox {
  width: 160rpx;
  height: 160rpx;
  margin: 20rpx 0 0 24rpx;
  cursor: pointer;
}
.addPicBox image {
  width: 100%;
  height: 100%;
}
/* 联系方式 */
.contactText {
  width: 224rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  text-align: left;
  color: #000000;
  margin: 34rpx 0 0 24rpx;
}
/* 提交电话 */
.phoneBox {
  width: 702rpx;
  height: 80rpx;
  background: #ffffff;
  border-radius: 10rpx;
  margin: 24rpx 0 0 24rpx;
  overflow: hidden;
}
.phone {
  margin: 24rpx 0 0 24rpx;
}
/* 提交 */
.submitBtn {
  width: 474rpx;
  height: 80rpx;
  background: #1c6a4f;
  border-radius: 40rpx;
  margin: 100rpx 0 0 138rpx;
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
  text-align: left;
  color: #ffffff;
  cursor: pointer;
  text-align: center;
  line-height: 80rpx;
}