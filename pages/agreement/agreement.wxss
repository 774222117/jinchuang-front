/* pages/agreement/agreement.wxss */
page{
  background-color: #fff;
  padding: 0 24rpx;
  width: 100%;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  padding-bottom: calc(24rpx + env(safe-area-inset-bottom))
}
.title{
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  font-weight: 700;
  margin: 24rpx 0;
}
.text{
  text-indent: 50rpx;
  font-size: 24rpx;
  line-height: 36rpx;
}
.title2{
  width: 100%;
  font-size: 26rpx;
  font-weight: 600;
  margin: 36rpx 0 0;
}
.text2{
  font-size: 24rpx;
  line-height: 36rpx;
}