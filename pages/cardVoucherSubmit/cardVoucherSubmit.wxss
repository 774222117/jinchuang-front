.addressBox {
  /* width: 702rpx; */
  margin: 24rpx auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 32rpx 14rpx 32rpx 24rpx;
  background: #ffffff;
  border-radius: 20rpx;

}

.addressBox_left {
  display: flex;
  align-items: center;
}

.addressBox_img {
  width: 74rpx;
  height: 74rpx;
