/* pages/cardVoucherSubmit/cardVoucherSubmit.wxss */
.top{
  width: 100%;
  height: 422rpx;
  background-color: #ffffff;
}
.top .header{
  width: 702rpx;
  height: 200rpx;
  padding-top: 6rpx;
  border-bottom: 2rpx solid rgba(226,226,226,1);
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
}
.top .header .header_imgs{
  width: 296rpx;
  height: 156rpx;
  border-radius: 8rpx;
}
.top .header .header_text{
  width: 224rpx;
  height: 156rpx;
}
.top .header .header_text .text1{
  font-size: 30rpx;
  color: #000000;
  margin-bottom: 6rpx;
}
.top .header .header_text .text2{
  font-size: 28rpx;
  color: rgba(0,0,0,0.53);
  margin-bottom: 30rpx;
}
.top .header .header_text .text3{
  font-size: 36rpx;
  color: rgba(243,84,55,1);
}
.top .header .header_label{
  width: 152rpx;
  height: 32rpx;
  border: 2rpx solid #e2b472;
  border-radius: 18rpx;
  font-size: 20rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  text-align: center;
  color: #e1ab5d;
  line-height: 30rpx;
  align-self: flex-end;
  margin-bottom: 37rpx;
}
/* 说明 */
.infos{
  width: 702rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  color: rgba(0,0,0,0.53);
  margin: 35rpx auto 63rpx;
}
/* 购买数量 */
.payNum{
  width: 702rpx;
  height: 52rpx;
  margin: 0 auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.payNum .lefts{
  width: 112rpx;
  height: 52rpx;
  font-size: 28rpx;
  color: #323232;
  line-height: 52rpx;
}
.payNum .rights{
  width: min-content;
  height: 52rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.payNum .rights .cut{
  width: 52rpx;
  height: 52rpx;
  font-size: 32rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: bold;
  text-align: center;
  color: #3d444d;
}
.payNum .rights .shopNum{
  min-width: 52rpx;
  height: 52rpx;
  text-align: center;
  line-height: 52rpx;
  padding: 0 18rpx;
}
.payNum .rights .add{
  width: 52rpx;
  height: 52rpx;
}
/* 商品合计 */
.shopTotal{
  width: 100%;
  height: 130rpx;
  padding: 0 24rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 20rpx auto 0;
  background-color: #ffffff;
}

.shopTotal.text1,.footer_price .text1{
  font-size: 30rpx;
  color: #000000;
  margin-bottom: 6rpx;
}
.shopTotal .text3,.footer_price .text3{
  font-size: 36rpx;
  color: rgba(243,84,55,1);
}

/* 底部 */
.footer{
  width: 100%;
  min-height: 124rpx;
  background: #ffffff;
  box-shadow: 0px -4rpx 12rpx 0px rgba(0,0,0,0.06); 
  position: fixed;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-left: 24rpx;
  padding-right: 24rpx;
}
.footer_price{
  display: flex;
  align-items: center;

}
.btms{
  width: 200rpx;
  height: 64rpx;
  background: #1c6a4f;
  border-radius: 10rpx;
  font-size: 28rpx;
  color: #ffffff;
  text-align: center;
  line-height: 64rpx;
}










